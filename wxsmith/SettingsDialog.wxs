				<pos>208,88</pos>
				<size>100,21</size>
			</object>
			<object class="wxButton" name="ID_BUTTON1" variable="btnSettingsCancel" member="yes">
				<label>Cancel</label>
				<pos>224,136</pos>
				<size>83,23</size>
				<handler function="OnbtnSettingsCancelClick" entry="EVT_BUTTON" />
			</object>
			<object class="wxButton" name="ID_BUTTON2" variable="btnSettingsOK" member="yes">
				<label>OK</label>
				<pos>128,136</pos>
				<size>83,23</size>
				<handler function="OnbtnSettingsOKClick" entry="EVT_BUTTON" />
			</object>
		</object>
	</object>
</wxsmith>
